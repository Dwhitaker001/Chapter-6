def main():
    # open a file named names.txt.

    infile_variable = open('names.txt', 'r')

# names_data = open('names.txt', 'r')

    line1 = infile_variable.readline()
    line2 = infile_variable.readline()
    line3 = infile_variable.readline()
    print(line1 + line2 + line3)

    infile_variable.close()


main()
